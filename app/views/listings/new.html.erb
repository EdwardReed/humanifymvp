<div id="listing-new" class="container text-center">
  <div class="row">
    <%= link_to @listing.type ? :back : root_path do %>
      <div id="back" class="panel col-sm-1 col-xs-offset-1">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
      </div>
    <% end %>
    <div id="listing-content" class="col-sm-8 panel panel-default">
      <div class="col-md-offset-2 col-md-8">
        <%= render 'listings/form' %>
      </div>
    </div>
  </div>
</div>
